{% extends 'base.html.twig' %}

{% block title %}Déposer votre attestation du stage{% endblock %}

{% block body %}
    <div class="mt-3">
        {{form_start(ficheEvaluationForm)}}
            <h1 >Déposer votre fiche d'évaluation du stage</h1>
            <div class="form-group">
                <label for="sujet">Titre</label>
                {{form_widget(ficheEvaluationForm.titre)}}
            </div>
            <div class="form-group">
                <label for="sujet">Déscription</label>
                {{form_widget(ficheEvaluationForm.description)}}
            </div>
            <div class="form-group">
                <label for="sujet">Fichier</label>
                {{form_widget(ficheEvaluationForm.fichier)}}
            </div>
            <div class="form-group">
                <input type="submit" class="btn btn-success col col-4 mt-3" value="Valider">
            </div>
        {{form_end(ficheEvaluationForm)}}
    </div>


{% endblock %}
